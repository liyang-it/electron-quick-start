{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "一个最基础的快速启动 Electron程序，实现了最基础常用功能",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.liyangit",
    "productName": "electronQuickStart",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "msi",
      "icon": "./assets/favicon(256).ico"
    }
  },
  "repository": "https://github.com/liyang-it/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start"
  ],
  "author": "liyangit",
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
